@model PieListViewModel
<h1>@Model.CurrentCategory</h1>

<div class="row">
        @foreach (var pie in Model.pies)
        {
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
            <img src="@pie?.ImageThumbnailUrl" alt="">
            <div class="caption">
                <h3>
                    <a>@pie.Name</a>
                </h3>
            </div>
        </div>
    </div>
        }
</div>
